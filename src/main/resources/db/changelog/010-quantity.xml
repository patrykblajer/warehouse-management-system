<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="1" author="patbla">
        <createTable tableName="quantity">
            <column autoIncrement="true" name="id" type="bigint">
                <constraints primaryKey="true"/>
            </column>
            <column name="available" type="double">
                <constraints nullable="true"/>
            </column>
            <column name="in_reservation" type="double">
                <constraints nullable="true"/>
            </column>
            <column name="stacked_on_pallet" type="double">
                <constraints nullable="true"/>
            </column>
            <column name="minimum_level_of_stocks" type="double">
                <constraints nullable="true"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>